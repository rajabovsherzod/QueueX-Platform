{
  "name": "e-queue-backend",
  "version": "1.0.0",
  "description": "E-Queue Multi-Tenant SaaS Backend API",
  "main": "dist/server.js",
  "scripts": {
    "predev": "pnpm exec prisma generate",
    "dev": "pnpm exec prisma generate && nodemon --exec ts-node -r tsconfig-paths/register src/server.ts",
    "build": "pnpm exec tsc",
    "start": "node dist/server.js",
    "postinstall": "pnpm exec prisma generate",
    "db:generate": "pnpm exec prisma generate",
    "db:push": "pnpm exec prisma db push",
    "db:migrate": "pnpm exec prisma migrate dev",
    "db:migrate:prod": "pnpm exec prisma migrate deploy",
    "db:seed": "pnpm exec ts-node prisma/seed.ts",
    "db:studio": "pnpm exec prisma studio",
    "db:reset": "pnpm exec prisma migrate reset",
    "test": "pnpm exec jest",
    "test:watch": "pnpm exec jest --watch",
    "lint": "pnpm exec eslint src/**/*.ts",
    "lint:fix": "pnpm exec eslint src/**/*.ts --fix",
    "clean": "rm -rf dist node_modules",
    "reinstall": "pnpm clean && pnpm install"
  },
  "keywords": [
    "queue",
    "saas",
    "multi-tenant",
    "prisma",
    "postgresql"
  ],
  "author": "E-Queue Team",
  "license": "MIT",
  "packageManager": "pnpm@8.15.0",
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "@types/cookie-parser": "^1.4.9",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "qrcode": "^1.5.3",
    "redis": "^4.6.12",
    "socket.io": "^4.7.4",
    "twilio": "^4.19.3",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^4.1.9"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.5",
    "@types/nodemailer": "^6.4.14",
    "@types/qrcode": "^1.5.5",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prisma": "^5.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
